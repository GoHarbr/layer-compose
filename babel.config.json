{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true,
          "browsers": "supports promises and supports async-functions and last 5 versions"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-proposal-throw-expressions",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
